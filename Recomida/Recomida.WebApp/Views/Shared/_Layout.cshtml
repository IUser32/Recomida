</head>
<body class="hold-transition skin-red sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>R</b>C</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Re</b>Comida</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->

                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                @Html.Partial("_LoginPartial")

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">

                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">Navegacion Principal</li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-home"></i> <span>Restaurantes</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="active"> @Html.ActionLink("Gestionar Restaurante", "Index", "Restaurantes")</li>
                            <li class="active"> @Html.ActionLink("Tipos", "Index", "TipoRestaurantes")</li>
                            <li class="active"> @Html.ActionLink("Ubicacion", "Index", "Ubicacion")</li>

                        </ul>
                    </li>

                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-apple"></i> <span>Comida</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="active">@Html.ActionLink("Tipos", "Index", "TipoComidas")</li>

                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <script src="~/Scripts/jquery-2.2.3.min.js"></script>
        <script src="~/Scripts/jquery-ui.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/app.min.js"></script>
        @RenderSection("scripts", required: false)
        <div class="content-wrapper">
            <section class="content">
                @RenderBody()
            </section>
            <hr />
        </div>
        <footer class="main-footer">
            <p>&copy; @DateTime.Now.Year - Adribel Gonzalez  - Luis Fortuna  - Samil Jimenez </p>
        </footer>
        </div>
   
</body>
</html>
